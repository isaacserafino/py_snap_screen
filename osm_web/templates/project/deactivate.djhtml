{% extends '../base.djhtml' %}

{% block content %}
	<h2>{% block title %}Delete Project{% endblock %}</h2>

	<p>Are you sure?</p>

	<form action="" method="post">
		{% csrf_token %}
	    <button type="submit" class="button">Delete</button>
	    <a class="button" href="{% url 'project-detail' object.slug %}">Cancel</a>
	</form>
{% endblock %}