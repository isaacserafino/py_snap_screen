{% extends '../base.djhtml' %}

{% block content %}
	<h2>{% block title %}Update Project{% endblock %}</h2>

	<p>Some text characters may be removed for quality.</p>

	<form action="" method="post">{% csrf_token %}
	    {{ form.as_p }}
	    <button type="submit" class="button">Update</button>
	    <a class="button" href="{% url 'project-detail' object.slug %}">Cancel</a>
	</form>
{% endblock %}